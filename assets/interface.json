{
    "name": "MaaGumballs",
    "url": "https://github.com/KhazixW2/MaaGumballs",
    "controller": [
        {
            "name": "ADB 默认方式",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服(默认)",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "启动",
            "doc": "直接启动游戏"
        },
        {
            "name": "每日收菜",
            "entry": "EntryEden",
            "doc": "每日收菜，需要在大地图主界面"
        },
        {
            "name": "联盟奖励",
            "entry": "UnionReward_Start",
            "doc": "一键完成联盟所有的任务，并领取联盟礼包奖励"
        },
        {
            "name": "黑永恒",
            "entry": "GoDownstairsTrick",
            "option": [
                "服务器"
            ],
            "doc": "使用黑下楼,需要V2ray应用"
        }
    ],
    "option": {
        "刷取次数": {
            "cases": [
                {
                    "name": "1",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 1,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                },
                {
                    "name": "2",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 2,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                },
                {
                    "name": "3",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 3,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                },
                {
                    "name": "4",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 4,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                },
                {
                    "name": "5",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 5,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                },
                {
                    "name": "10",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 10,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                },
                {
                    "name": "20",
                    "pipeline_override": {
                        "TL01_Start_Count": {
                            "custom_action_param": {
                                "count": 0,
                                "target_count": 20,
                                "next_node": [
                                    "BackButton"
                                ]
                            }
                        }
                    }
                }
            ]
        },
        "服务器": {
            "cases": [
                {
                    "name": "官服（默认）",
                    "pipeline_override": {
                        "Opengumballs_Auto": {
                            "action": "StartApp",
                            "package": "com.leiting.gumballs.leiting"
                        }
                    }
                },
                {
                    "name": "小米服",
                    "pipeline_override": {
                        "Opengumballs_Auto": {
                            "action": "StartApp",
                            "package": "com.leiting.gumballs.mi"
                        }
                    }
                }
            ]
        }
    }
}